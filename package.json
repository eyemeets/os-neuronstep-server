{
  "name": "os-neuronstep-server",
  "version": "1.0.0",
  "description": "A course generator using gpt-4o-mini",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --max_old_space_size=4096 --respawn --transpile-only --exit-child --watch tsc src/index.ts src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "NODE_OPTIONS=\"--loader ts-node/esm --no-warnings\" vitest run --watch --passWithNoTests",
    "dev:ship": "vercel dev",
    "ship": "pnpm build && vercel --prod",
    "v:login": "vercel login",
    "setup:eslint": "npx eslint --init"
  },
  "keywords": [],
  "author": {
    "name": "Daniel Hendricks",
    "email": "daniel@eyemeets.com",
    "url": "https://eyemeets.com"
  },
  "license": "ISC",
