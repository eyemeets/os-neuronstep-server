    "async-retry": "^1.3.3",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-file-routing": "^3.0.3",
    "express-fileupload": "^1.5.1",
    "jsdom": "^25.0.1",
    "multer": "1.4.5-lts.1",
    "openai": "^4.63.0",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^5.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/body-parser": "^1.19.5",
